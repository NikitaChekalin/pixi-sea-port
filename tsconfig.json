  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "ES6",
    "allowUnreachableCode": false,
    "noImplicitReturns": true,
    "noImplicitAny": true
  },
  "include": ["./**/*"],
  "exclude": ["./plugins/**/*", "./typings/**/*", "./dist/**/*"]
}
